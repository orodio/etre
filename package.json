{
  "name": "etre",
  "version": "0.0.1",
  "description": "Pattern Matching",
  "main": "dst/index.min.js",
  "scripts": {
    "build:min": "./node_modules/.bin/babel src | uglifyjs --screw-ie8 -mc -o dst/etre.min.js",
