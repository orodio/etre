  "license": "MIT",
  "devDependencies": {
    "babel": "5.8.23",
    "babel-plugin-lodash": "0.2.0",
    "babel-plugin-uglify": "1.0.2",
    "mocha": "2.3.3",
    "uglify-js": "^2.4.24"
  },
  "dependencies": {
    "lodash": "3.10.1"
  }
}
